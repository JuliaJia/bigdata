package algorithm.dataStructure.case2;

public class Node {
	//÷∏’Î”Ú
	private Node befour;
	private Node next;
	
	private int data;

	
	Node(){
		befour = null;
		next = null;
	}
	
	Node(int it){
		data = it;
	}
	
	public Node next(){
		return next;
	}
	
	public Node setNext(Node nextval){
		return next = nextval;
	}
	
	public Node befour(){
		return befour;
	}
	
	public Node setBefour(Node befourval){
		return befour = befourval;
	}
	
	public int data(){
		return data;
	}
	
	public int setData(int it){
		return data = it;
	}

	public void display(){
		System.out.println(data + " ");
	}
}
