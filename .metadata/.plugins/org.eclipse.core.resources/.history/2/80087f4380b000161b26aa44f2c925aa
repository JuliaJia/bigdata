package algorithm.dataStructure.doubleLinkedList1;



public class TestDLinkedList {
	
	public static Node first;

	public static void main(String[] args) {
		addBefour(5);
		addBefour(4);
		addBefour(3);
		addBefour(2);
		addBefour(1);
		
		displayAllNodes();

	}
	
	
	public static void addBefour(int it){
		Node curr = new Node();
		Node newNode = new Node(curr.next(),curr,it);
		curr.setNext(newNode);
		if(curr.next().next() != null){
			curr.next().next().setBefour(curr.next());
			
		}
	}
	
	
	public static void displayAllNodes(){
		Node current = first;
		while(current != null){
			current.display();
			current = current.next;
		}
		System.out.println();
	}

}

