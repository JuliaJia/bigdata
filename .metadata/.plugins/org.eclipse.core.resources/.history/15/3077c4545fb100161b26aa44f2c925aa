package algorithm.dataStructure.case1;



public class Dlink {
	public Node head;  //Õ∑÷∏’Î
	
	Dlink(){
		this.head = null;
	}
	
	Dlink(Node head){
		this.head = head;	
	}

	
	
	public void addBefore(int n,int a){
		Node curr = new Node(n,a);
		if(head == null){
			head = curr;
			head.setNext(null);
			curr = curr.next();
			
		}else{
			head = curr;
			head.setNext(null);
			curr = curr.next();
			
		}
		
	}
	
	/*public String addPolynominalOne(){
		return polynominalOne;
	}*/
	
	
	public void displayAll(){
		Node curr = head;
		if(head != null){
			while(curr != null){
				curr.display();
				curr = curr.next();
			}
		}
		
	}
	

}
