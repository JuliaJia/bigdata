package algorithm;

public class ON2 {

	public static void main(String[] args) {
		int[] A = {-2,-3,4,5,6,7,-19,20,45,-10,11,-23,58,-14,78,-90,100,-89,-37,21,90,-88,67,54,-33};
		System.out.println(on2(A));

	}
	
	private static int on2(int[] numbers){
		int maxSum = 0;
		int i,j;
		int thisSum;
		for(i = 0; i < numbers.length;i++){
			thisSum = 0;
			for(j = i ; j < numbers.length ; j++){
				thisSum += numbers[j];
				if(thisSum > maxSum){
					maxSum = thisSum;
				}
			}
			
		}
		
		return maxSum;
	}

}
