package algorithm.dataStructure.stack2;



public class Linked {
	
	public Node head;

	public int count;
	
	public Linked(){
		head = null;

		count = 0;
	}
	
	public Node stackPop(Node stack){
		stack = head;
		head = stack.next();
		count--;
		return stack;
	}
	
	public void stackPush(Object data){
		Node node = new Node(data);
		node.setNext(head);
		head = node;
	}
	
	public int length(){
		return count;
	}
	
	public void displayAllNodes(){
		Node current = head;
		while(current != null){
			current.display();
			current = current.next();
			
		}
		System.out.println();
	}

}
