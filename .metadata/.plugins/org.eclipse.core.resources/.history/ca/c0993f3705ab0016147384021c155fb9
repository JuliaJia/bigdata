package algorithm;

import javax.security.auth.kerberos.KerberosKey;

public class TimeCount {

	public static void main(String[] args) {
		timeCount(20);

	}
	
	public static void timeCount(int i){
		int count = 0;
		if(i > 1){
			timeCount(i/2);
			count++;
		}
		System.out.println(i / 2);
		count++;
		System.out.println(count);

		
		
	}

	
}
